{
  "name": "rougin/dexter",
  "description": "\"Ready-to-eat\" CRUD code in PHP.",
  "keywords": [ "php-crud", "php-depot", "php-rapid" ],
  "homepage": "https://roug.in/dexter/",
  "license": "MIT",
  "authors":
  [
    {
      "email": "rougingutib@gmail.com",
      "homepage": "https://roug.in/",
      "name": "Rougin Gutib",
      "role": "Software Engineer"
    }
  ],
  "require":
  {
    "php": ">=5.3.0",
    "illuminate/database": "*",
    "psr/http-message": "~1.0",
    "rougin/slytherin": "~0.9"
  },
  "require-dev":
  {
    "phpunit/phpunit": "*",
    "sanmai/phpunit-legacy-adapter": "*"
  },
  "autoload":
  {
    "psr-4":
    {
      "Rougin\\Dexter\\": "src"
    }
  },
  "autoload-dev":
  {
    "psr-4":
    {
      "Rougin\\Dexter\\": "tests"
    }
  },
  "scripts":
  {
    "analyze": "phpstan analyse",
    "restyle": "php-cs-fixer fix --config=phpstyle.php",
    "test": "phpunit"
  }
}
