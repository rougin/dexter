{
    "name": "rougin/dexterity",
    "description": "\"Ready-to-eat\" and framework agnostic CRUD controllers.",
    "keywords": ["agnostic", "crud", "controllers", "dexterity", "ready-to-eat"],
    "homepage": "https://github.com/rougin/dexterity",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "https://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "psr/http-message": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2|~5.7",
        "scrutinizer/ocular": "~1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Rougin\\Dexterity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rougin\\Dexterity\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}